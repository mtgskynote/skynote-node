    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "adm-zip": "^0.5.16",
    "ajv": "^8.16.0",
    "axios": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.0",
    "buffer": "^6.0.3",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fs": "^0.0.1-security",
