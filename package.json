{
  "name": "skynote",
  "version": "1.0.0",
  "description": "#### Track Your Job Search",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "install-dependencies": "npm run install-client && npm install",
    "setup-production": "npm run install-client && npm run build-client && npm install",
    "install-client": "cd client-jb && npm install",
    "build-client": "cd client-jb && npm run build",
    "server": "nodemon server",
    "client": "PORT=3000 npm start --prefix client-jb",
    "start": "concurrently  --kill-others-on-fail  \"npm run server\" \"npm run client\" ",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fix:eslint": "eslint . --fix",
    "format": "prettier --write .",
    "fix:prettier": "prettier --write .",
    "fix": "npm run format && npm run lint:fix",
    "test": "mocha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@eslint/js": "^9.7.0",
    "chai": "^5.1.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.4",
    "globals": "^15.8.0",
    "mocha": "^10.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "sinon": "^18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
